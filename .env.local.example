# Census API Configuration
# Get your free API key from: https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_api_key_here

# YouTube API Configuration
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Find your channel ID: https://www.youtube.com/account_advanced
YOUTUBE_API_KEY=your_youtube_api_key_here
YOUTUBE_CHANNEL_ID=your_channel_id_here

# Google Maps API Configuration
# Get your API key from: https://console.cloud.google.com/google/maps-apis
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Analytics Configuration
# Get your Measurement ID (G-XXXXXXXXXX) from: https://analytics.google.com/
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Google Tag Manager Configuration
# Get your Container ID (GTM-XXXXXXX) from: https://tagmanager.google.com/
NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# Google Ads Conversion Tracking
# Get your Conversion ID and Label from: Google Ads > Tools > Conversions
NEXT_PUBLIC_GOOGLE_ADS_CONVERSION_ID=AW-XXXXXXXXXX
NEXT_PUBLIC_GOOGLE_ADS_CONVERSION_LABEL=your_conversion_label_here

# Facebook Pixel Configuration
# Get your Pixel ID from: https://business.facebook.com/events_manager
NEXT_PUBLIC_FACEBOOK_PIXEL_ID=your_facebook_pixel_id_here

# Sanity Configuration
# Project ID and dataset (defaults to 1zb39xqr / production if not set)
NEXT_PUBLIC_SANITY_PROJECT_ID=your_sanity_project_id_here
NEXT_PUBLIC_SANITY_DATASET=production
# Optional: Only needed if your dataset is not public
# Get a read token from: https://www.sanity.io/manage > Your Project > API > Tokens
NEXT_PUBLIC_SANITY_TOKEN=your_sanity_read_token_here

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://yourdomain.com

# Cloze CRM Integration
# To get API access, email support@cloze.com with your integration plans
# Documentation: https://help.cloze.com/category/1651-api
CLOZE_API_KEY=your_cloze_api_key_here
CLOZE_USER_EMAIL=your_cloze_account_email@example.com
